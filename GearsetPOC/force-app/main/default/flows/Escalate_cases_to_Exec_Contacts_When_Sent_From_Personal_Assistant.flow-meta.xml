    <description>Escalate cases to Exec Contacts When Sent From Personal Assistant</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>NOT(ISBLANK([EmailMessage].ParentId)) &amp;&amp; 
LOWER([EmailMessage].ToAddress) = &quot;exec.tc@eurostar.com&quot; &amp;&amp;
NOT(ISPICKVAL([EmailMessage].Parent.Status, &quot;Closed&quot;)) &amp;&amp;
NOT(ISPICKVAL([EmailMessage].Parent.Type, &quot;Duty Travel&quot;)) &amp;&amp; /*!!!!!!!!!!!!!!!!!*/
NOT(CONTAINS($Setup.Exec_Cases_Escalation_Rules__c.Exclusion_Owners_IDs__c, 
                             LEFT([EmailMessage].Parent.OwnerId, 15)) ) &amp;&amp;
(
 CONTAINS($Setup.Exec_Cases_Escalation_Rules__c.Senders_Part_1__c, LOWER([EmailMessage].FromAddress)) ||
 CONTAINS($Setup.Exec_Cases_Escalation_Rules__c.Senders_Part_2__c, LOWER([EmailMessage].FromAddress)) ||
 CONTAINS($Setup.Exec_Cases_Escalation_Rules__c.Senders_Part_3__c, LOWER([EmailMessage].FromAddress)) ||
 CONTAINS(LOWER([EmailMessage].FromAddress),
                                     $Setup.Exec_Cases_Escalation_Rules__c.Sender_Domain_1__c)
)

</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
