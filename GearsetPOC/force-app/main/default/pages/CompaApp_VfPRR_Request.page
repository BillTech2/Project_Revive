<apex:page showHeader="false" sidebar="false" language="{!selectedLanguageCode}" controller="CompApp_CtrlPRR_Request" standardStylesheets="false" applyHtmlTag="false" applyBodyTag="false">

        <div id="make-a-claim-controller-wrapper">

            <h1 class="row">{!$label.CompApp_prrComp}</h1>
            <div class="row p1">{!$label.CompApp_enterDetails}</div>

            <div class="row p1">{!$label.CompApp_required_fields}</div>

            <h2 class="row">{!$label.CompApp_Traveller_details}</h2>

            <div class="row {{formManager.firstNameValdity | validityClass}}">
                <label for="first-name" class="cell-label">{!$label.CompApp_First_name}</label>
                <div class="input-wrapper validity-sign cell-input">
                    <input
                        
                        placeholder="{!$label.CompApp_Traveller_First_Name}" 
                        id="first-name"
                        ng-pattern="USER_NAME_REGEX"
                        ng-change="formManager.firstNameValdity = !! $angularState.personalInfo.firstName"
                        ng-model="$angularState.personalInfo.firstName"
                    /> 
                    <div class="error-message">{!$label.CompApp_enterFirstNamePRR}</div>
                </div>
            </div>

            <div class="row {{formManager.lastNameValdity | validityClass}}">
                <label for="surname" class="cell-label">{!$label.CompApp_Surname}</label>
                <div class="input-wrapper validity-sign cell-input">
                    <input 
                    maxlength="30"
                        placeholder="{!$label.CompApp_Traveller_Surname}" 
                        id="surname"
                        ng-pattern="USER_30_NAME_REGEX"
                        ng-change="formManager.lastNameValdity = !! $angularState.personalInfo.lastName" 
                        ng-model="$angularState.personalInfo.lastName"
                    />
                    <div class="error-message">{!$label.CompApp_enterLastNamePRR}</div>
                </div>
            </div>

            <div class="row {{formManager.primaryEmailValdity | validityClass}}">
                <label for="email" class="cell-label">{!$label.CompApp_email}</label>
                <div class="input-wrapper validity-sign cell-input">
                    <input type="email"
                        placeholder="{!$label.CompApp_Traveller_Email}" 
                        id="email"
                        ng-change="formManager.primaryEmailValdity = isPrimaryEmailValid()" 
                        ng-model="formManager.primaryEmail"
                    /> 
                    <div class="error-message">{!$label.CompApp_enterEmailPRR}</div>
                </div>
            </div>

            <div class="row {{formManager.secondaryEmailValdity | validityClass}}">
                <label for="email" class="cell-label">{!$label.CompApp_Confirm_email}</label>
                <div class="input-wrapper validity-sign cell-input">
                    <input type="email"
                        placeholder="{!$label.CompApp_Traveller_Email}" 
                        id="email"
                        ng-paste="preventPaste($event)"
                        ng-change="formManager.secondaryEmailValdity = isSecondaryEmailValid()" 
                        ng-model="formManager.secondaryEmail"
                    /> 
                    <div class="error-message">{!$label.CompApp_confirmEmailPRR}</div>
                </div>
            </div>

            <div class="row">
                <label for="country-of-residence" class="cell-label">{!$label.CompApp_CountryOfResidence}</label>
                <div class="input-wrapper validity-sign cell-input">
                    <select
                        ng-model="$angularState.financialInfo.countryOfResidency"
                    >
                        <option value="United Kingdom">{!$label.CompApp_UnitedKingdom}</option>
                        <option value="Within European Union">{!$label.CompApp_WithinEuropeanUnion}</option>
                        <option value="Rest of the World">{!$label.CompApp_RestOfTheWorld}</option>
                    </select>
                    <div class="error-message">{!$label.CompApp_selectResidenceCountry}</div>
                </div>
            </div>


            <h2 class="row">{!$label.CompApp_booking_details}</h2>

            <div class="row tooltip {{formManager.TCNValdity | validityClass}} ">
                <label for="tcn" class="cell-label">{!$label.CompApp_Ticket_number}</label> 
                <div class="input-wrapper validity-sign cell-input">
                    <input type="number"
                        onkeyup="angular.element(this).scope().applyTCNChange($(this).val());" 
                        placeholder="{!$label.CompApp_nine_digit_ticket}" 
                        id="tcn"
                    />
                    <a class="tooltip-sign" id="tcn-tooltip"></a>
                    <div class="error-message">{!$label.CompApp_enterTCNPRR}</div>
                </div>
            </div>

            <div class="row tooltip {{formManager.PNRValdity | validityClass}}">
                <label for="pnr" class="cell-label">{!$label.CompApp_Booking_reference}</label> 
                <div class="input-wrapper validity-sign cell-input">
                    <input 
                        placeholder="{!$label.CompApp_six_letter_booking}" 
                        id="pnr"  
                        ng-change="$angularState.travel.PNR = $angularState.travel.PNR.toUpperCase();formManager.PNRValdity = isPNRValid()" 
                        ng-model="$angularState.travel.PNR"
                    />
                    <a class="tooltip-sign" id="pnr-tooltip"></a>
                    <div class="error-message">{!$label.CompApp_enterPNRPRR}</div>
                </div>
            </div>

            <div class="row tooltip {{formManager.isRequirePaymentInfoValdity | validityClass}}">
                <label for="use-existing-payment" class="cell-label">{!$Label.CompApp_HaveYouBookedOnEurostar}</label>
                <div class="input-wrapper validity-sign cell-input">
                    <select
                        ng-model="$angularState.financialInfo.isRequirePaymentInfo"
                        ng-change="formManager.isRequirePaymentInfoValdity = $angularState.financialInfo.isRequirePaymentInfo !== 'unseletcted'" 

                    >
                        <option value="unseletcted">{!$Label.CompApp_pleaseSelectValue}</option>
                        <option value="false">{!$Label.CompApp_UseMyExisitngCare}</option>
                        <option value="true">{!$Label.CompApp_EnterNewRefundInfo}</option>
                    </select>
                    <a class="tooltip-sign" id="existing-payment-tooltip"></a>
                    <div class="error-message">{!$Label.ComApp_PleaseSelectOption}</div>
                </div>
            </div>

            <span class="ng-cloak" ng-show="$angularState.financialInfo.isRequirePaymentInfo === 'true'">
                <h2 class="row">{!$label.CompApp_accountDetails}</h2>

                <div class="row tooltip {{formManager.bankAccountNameValidity | validityClass}} " ng-show="$angularState.financialInfo.countryOfResidency === 'United Kingdom' || $angularState.financialInfo.countryOfResidency === 'Within European Union'">
                    <label for="ban" class="cell-label">{!$label.CompApp_BankAccountName}</label>
                    <div class="input-wrapper validity-sign cell-input">
                        <input
                                placeholder="{!$label.CompApp_BankAccountName}"
                                id="ban"
                                ng-change="formManager.bankAccountNameValidity = isBankAccountNameValid() "
                                ng-model="$angularState.financialInfo.bankAccountName"
                        />
                        <div class="error-message">{!$label.CompApp_enterBankAccountName}</div>
                    </div>
                </div>

                <div class="row tooltip {{formManager.bankAccountValidity | validityClass}} " ng-show="$angularState.financialInfo.countryOfResidency === 'United Kingdom'">
                    <label for="ban" class="cell-label">{!$label.CompApp_BankAccountNumber}</label> 
                    <div class="input-wrapper validity-sign cell-input">
                        <input
                            placeholder="{!$label.CompApp_BankAccountNumber}"     
                            id="ban"
                            ng-change="formManager.bankAccountValidity = isBankAccountValid() " 
                            ng-model="$angularState.financialInfo.bankAccountNumber" 
                        />
                        <div class="error-message">{!$label.CompApp_enterBankAccountNumber}</div>
                    </div>
                </div>
                
                <div class="row tooltip {{formManager.bankSortCodeValidity | validityClass}} " ng-show="$angularState.financialInfo.countryOfResidency === 'United Kingdom'">
                    <label for="ban" class="cell-label">{!$label.CompApp_BankSortCode}</label> 
                    <div class="input-wrapper validity-sign cell-input">
                        <input
                            placeholder="{!$label.CompApp_BankSortCode}"     
                            id="ban"
                            ng-change="formManager.bankSortCodeValidity = isBankSortCodeValid()"
                            ng-model="$angularState.financialInfo.bankSortCode"  
                        />
                        <div class="error-message">{!$label.CompApp_enterBankSortCode}</div>
                    </div>
                </div>

                <div class="row tooltip {{formManager.IbanNumberValidity | validityClass}} " ng-show="$angularState.financialInfo.countryOfResidency === 'Within European Union'">
                    <label for="ban" class="cell-label">{!$label.CompApp_IBAN}</label> 
                    <div class="input-wrapper validity-sign cell-input">
                        <input
                            placeholder="{!$label.CompApp_IBAN}"     
                            id="ban"
                            ng-change="formManager.IbanNumberValidity = isIbanNumberValid()" 
                            ng-model="$angularState.financialInfo.IbanNumber"  
                        />
                        <div class="error-message">{!$label.CompApp_enterIBAN}</div>
                    </div>
                </div>

                <div class="row tooltip {{formManager.bankBIC_SwiftCodeValidity | validityClass}} " ng-show="$angularState.financialInfo.countryOfResidency === 'Within European Union'">
                    <label for="ban" class="cell-label">{!$label.CompApp_Bank}</label> 
                    <div class="input-wrapper validity-sign cell-input">
                        <input
                            placeholder="{!$label.CompApp_Bank}"     
                            id="ban"
                            ng-change="formManager.bankBIC_SwiftCodeValidity = isBankSwiftCodeValid()" 
                            ng-model="$angularState.financialInfo.bankBIC_SwiftCode"  
                        />
                        <div class="error-message">{!$label.CompApp_enterBank}</div>
                    </div>
                </div>


                <div class="row tooltip {{formManager.PAYPALValdity | validityClass}} " ng-show="$angularState.financialInfo.countryOfResidency === 'Rest of the World'">
                    <label for="paypal" class="cell-label">{!$label.CompApp_Paypal}</label> 
                    <div class="input-wrapper validity-sign cell-input">
                        <input type="email" 
                            placeholder="{!$label.CompApp_Paypal}"    
                            id="paypal"
                            ng-change="formManager.PAYPALValdity = isPAYPALValid()" 
                            ng-model="$angularState.financialInfo.paypalAccount"   
                        />
                        <div class="error-message">{!$label.CompApp_enterPaypal}</div>
                    </div>
                </div>
                
            </span>

            <div class="row {{ formManager.captchaValdity | validityClass }}">
                <label for="recaptcha" class="cell-label" >{!$label.CompApp_robot_question}</label>
                <div class="cell-input">
                    <no-captcha
                        g-recaptcha-response="captchaResponse"
                        theme='light'
                        control="noCaptchaControl"
                        site-key="{!GOOGLE_CAPTCHA_SITE_KEY}"
                    >
                    </no-captcha>
                    <div class="error-message">{!$label.CompApp_Validate_not_robot}</div>
                </div>
            </div>

            <div class="row commands-section">
                <a href="" class="btn primary" ng-click="submitPRRClaim();" id="prr-request-button">{!$label.CompApp_Eligibility_Check}</a>
                <p class="error-message-main" ng-hide="formManager.noErrors">{!$label.CompApp_incorrectDataPrr}</p>
                <div class="lds-spinner" style="margin-left: -15px" hidden="hidden"><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div></div>
            </div>

            <script>
                var GLOBAL_REMOTE_USER_IP = '{!USER_IP}';
            </script>
        </div> <!-- END OF make-claim controller -->    
        
