<apex:page standardController="Case" recordSetVar="cases" extensions="AssignCaseToQueueExtension" action="{!changeOwner}" showHeader="false" sidebar="false">
    <apex:slds />
    <div class="slds-scope">
        <apex:form id="form" style="height: 200px;">
            <!-- SPINNER -->
                    <div id="spinner" class="slds-spinner_container slds-is-relative" style="top:200%">
                        <div role="status" class="slds-spinner slds-spinner--large slds-spinner--brand">
                            <div class="slds-spinner__dot-a"></div>
                            <div class="slds-spinner__dot-b"></div>
                        </div>
                    </div>
            <!-- / SPINNER -->
        </apex:form>
    </div>
</apex:page>