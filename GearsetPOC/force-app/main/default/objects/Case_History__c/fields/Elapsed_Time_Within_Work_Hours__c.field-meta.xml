<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Elapsed_Time_Within_Work_Hours__c</fullName>
    <externalId>false</externalId>
    <formula>if(
isblank(Exit_Time__c),
((Now()  -   Entry_Time_Within_Work_Hours__c )*1440 ),
((Exit_Time__c  -   Entry_Time_Within_Work_Hours__c)*1440 )
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Elapsed Time Within Work Hours</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
