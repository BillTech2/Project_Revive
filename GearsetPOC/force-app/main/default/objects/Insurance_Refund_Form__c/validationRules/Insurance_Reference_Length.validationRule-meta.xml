<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Insurance_Reference_Length</fullName>
    <active>true</active>
    <errorConditionFormula>LEN(Name)&lt;&gt;15</errorConditionFormula>
