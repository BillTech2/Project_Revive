<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Reason_for_Contact_is_required</fullName>
    <active>false</active>
    <errorConditionFormula>$Profile.Name &lt;&gt; &quot;Integration Profile&quot; &amp;&amp; AND(
OR(RecordTypeId=&quot;0120N000000sdhC&quot;, RecordTypeId=&quot;0123z000000lIGz&quot;),
ISBLANK(TEXT(Reason_For_Contact_Dynamic_Package__c)),
ISBLANK(TEXT(Initial_Reason_For_Contact__c))
)</errorConditionFormula>
    <errorMessage>Please complete either Reason for Contact or Initial Reason for Contact (for outbound calling)</errorMessage>
</ValidationRule>
