<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Language_Code__c</fullName>
    <externalId>false</externalId>
    <formula>IF(OR(ISPICKVAL(Preferred_Language__c,&quot;English&quot;),ISPICKVAL(Preferred_Language__c,&quot;English (UK)&quot;)),&quot;en-gb&quot;,
 IF(ISPICKVAL(Preferred_Language__c,&quot;Dutch&quot;),&quot;nl&quot;,
 IF(ISPICKVAL(Preferred_Language__c,&quot;French&quot;),&quot;fr-ca&quot;,
 IF(ISPICKVAL(Preferred_Language__c,&quot;German&quot;),&quot;de&quot;,
NULL)

)))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Language Code</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
