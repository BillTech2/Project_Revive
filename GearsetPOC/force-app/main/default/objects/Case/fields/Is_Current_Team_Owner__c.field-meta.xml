<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Is_Current_Team_Owner__c</fullName>
    <externalId>false</externalId>
    <formula>IF(ISBLANK($User.ManagerId),
  IF(Team_Owner__c = $User.FirstName+&quot; &quot;+$User.LastName, 1, 0),
  IF($User.ManagerId = Owner:User.ManagerId, 1, 0)
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Team Owner Flag</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
