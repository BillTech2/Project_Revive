<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Social_SLA_Formula__c</fullName>
    <externalId>false</externalId>
    <formula>(IF 
(ISPICKVAL(Status, &quot;Closed&quot;),&quot;Closed&quot;, 
(IF 
((((ISPICKVAL(Origin, &quot;Facebook&quot;) &amp;&amp; Days_Since_Last_Modified__c &gt;= 30) 
|| 
(ISPICKVAL(Origin, &quot;Twitter&quot;) &amp;&amp; Days_Since_Last_Modified__c &gt;= 15) 
)), 
&quot;Over SLA&quot;, &quot;Within SLA&quot;))))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Social SLA</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
