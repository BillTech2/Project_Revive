<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Account_Status_Contracted</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
!($User.B2b_Bypass_Validation_Rules__c),
ISNEW(),
AccountId__r.RecordType.DeveloperName = &#39;Corporate_Account&#39;,
TEXT(AccountId__r.Account_Status__c) != &#39;Contracted&#39;
)</errorConditionFormula>
    <errorMessage>Account Status must be Contracted</errorMessage>
</ValidationRule>
