<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Ready_to_Auto_Close__c</fullName>
    <formula>AND(TODAY()&gt;= ADDMONTHS(IF(ISNULL(LastActivityDate), EnrollmentDate, LastActivityDate),12 * Program.Loyalty_Account_Closing_in_Years__c),
				TEXT(MemberStatus) != &#39;Inactive&#39;
			)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Ready to Auto Close</label>
    <trackHistory>false</trackHistory>
    <type>Checkbox</type>
</CustomField>
