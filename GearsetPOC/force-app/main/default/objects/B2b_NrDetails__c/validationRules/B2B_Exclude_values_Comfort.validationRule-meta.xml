<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>B2B_Exclude_values_Comfort</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
NOT($User.B2b_Bypass_Validation_Rules__c),
NOT($Permission.Sales_Admin),
OR(
CONTAINS(TEXT(B2b_Percentage_Discount_Comfort__c), &#39;9&#39;),
CONTAINS(TEXT(B2b_Percentage_Discount_Comfort__c), &#39;11&#39;),
CONTAINS(TEXT(B2b_Percentage_Discount_Comfort__c), &#39;13&#39;)
)
)</errorConditionFormula>
    <errorMessage>Percentage is invalid, it cannot be 1%, 2%, 3%, 4%, 9%, 11%, 13% or more than 20%</errorMessage>
</ValidationRule>
