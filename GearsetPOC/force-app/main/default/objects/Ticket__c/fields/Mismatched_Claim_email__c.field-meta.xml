<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Mismatched_Claim_email__c</fullName>
    <formula>IF(
(( LOWER(FraudCaseReference__r.SuppliedEmail) = LOWER(ETAP_Email__c))
|| ( LOWER(FraudCaseReference__r.SuppliedEmail) = LOWER(Email__c))
|| ( LOWER(FraudCaseReference__r.SuppliedEmail) = LOWER(Booker__r.PersonEmail)))
,False,True)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Mismatched Claim email</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
