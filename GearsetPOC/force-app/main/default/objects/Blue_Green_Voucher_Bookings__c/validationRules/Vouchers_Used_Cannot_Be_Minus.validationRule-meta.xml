<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Vouchers_Used_Cannot_Be_Minus</fullName>
    <active>true</active>
    <errorConditionFormula>CONTAINS(TEXT(Total_Number_of_Vouchers_Used__c),&quot;-&quot;)</errorConditionFormula>
    <errorMessage>Number of vouchers used cannot be minus</errorMessage>
</ValidationRule>
