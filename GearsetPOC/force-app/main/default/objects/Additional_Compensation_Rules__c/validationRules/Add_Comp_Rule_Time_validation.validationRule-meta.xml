<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Add_Comp_Rule_Time_validation</fullName>
    <active>false</active>
    <errorConditionFormula>(AND(AND(NOT(ISNULL(Time_From__c)),NOT(ISNULL(Time_To__c))),Time_From__c&gt;Time_To__c))</errorConditionFormula>
    <errorDisplayField>Time_From__c</errorDisplayField>
    <errorMessage>Time from should be less than Time to</errorMessage>
</ValidationRule>
