<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Handling_Time_For_Reporting__c</fullName>
    <externalId>false</externalId>
    <formula>if( 
isblank(CloseDateTime), 
((Now() - AcceptDateTime )*1440 ), 
((CloseDateTime - AcceptDateTime)*1440 ) 
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Handling Time (For Reporting)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
